<Window x:Class="uchetsredstv.Windows.AddSredstvo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:uchetsredstv.Windows"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="AddSredstvo" Height="450" Width="800">
    <Grid>
        <UniformGrid Rows="5" Columns="2">
            <Label>Инвентарный номер</Label>
            <TextBox Name="TbInventNomer"></TextBox>
            <Label>Модель</Label>
            <TextBox Name="TbModel"></TextBox>
            <Label>Дата приобретения</Label>
            <TextBox Name="TbData"></TextBox>
            <Label>Стоимость</Label>
            <TextBox Name="TbPrice"></TextBox>
            <Button Name="AddSredstvoButton" Click="AddSredstvoButton_Click">Добавить</Button>
            <Button Name="EditSredstvoButton" Click="EditSredstvoButton_Click">Изменить</Button>
        </UniformGrid>
    </Grid>
</Window>
